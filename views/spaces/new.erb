<form action='/spaces' method='post'>
  <div class="space_input_wrapper">
    <input class="space_input" type='text' name='name' placeholder='name' /> <br>
    <textarea rows="6" cols="30" class="space_input" id='description' type='text' name='description' placeholder='describe your space'> </textarea> <br>
    <input class="space_input" type-'text' name='price' placeholder='price per night' /> <br>
    <input class="space_input" type='date' name='start_date'/> <br>
    <input class="space_input" type='date' name='end_date'/> <br>
  </div>
  <input class="button" type='submit' value='Submit' />
</form>
